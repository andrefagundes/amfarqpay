{
  "hosting": {
    "public": "functions/docs",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"]
  },
  "functions": {
    "source": "functions"
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080,
      "import": "functions/emulator-data"
    },
    "ui": {
      "enabled": true
    }
  }
}
